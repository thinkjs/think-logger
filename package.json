{
  "name": "think-logger3",
  "version": "0.0.4",
  "description": "logger for ThinkJS 3.x",
  "main": "./lib",
  "scripts": {
    "compile": "babel src/ --out-dir lib/",
    "test": "npm run eslint && npm run test-cov",
    "test-cov": "nyc ava test/ && nyc report --reporter=html",
    "eslint": "eslint ./src",
    "prepublish": "npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkjs/think-logger.git"
  },
  "keywords": [
    "thinkjs",
    "logger",
    "log4js"
  ],
  "author": "lizheming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thinkjs/think-logger/issues"
  },
  "homepage": "https://github.com/thinkjs/think-logger#readme",
  "dependencies": {
    "log4js": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-think-node": "0.0.8",
    "eslint": "^3.16.1",
    "muk-require": "^1.0.0",
    "nyc": "^10.1.2"
  },
  "babel": {
    "presets": [
      "think-node"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
